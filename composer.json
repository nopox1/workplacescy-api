{
    "name": "workplacescy/api",
    "type": "project",
    "description": "REST API microservice of Workplaces in Cyprus website",
    "keywords": [
        "REST",
        "API",
        "microservice",
        "specialty",
        "work",
        "place",
        "cyprus"
    ],
    "homepage": "https://workplaces.cy/",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir",
            "homepage": "https://t.me/vladimir_mvs",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "workplacescy@gmail.com",
        "issues": "https://github.com/workplacescy/api/issues",
        "docs": "https://@TODO"
    },
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "ext-sqlite3": "*",
        "ext-phar": "*",
        "alexpechkarev/google-maps": "*",
        "area17/twill": "^3.0",
        "laravel/framework": "^9.0",
        "laravel/scout": "*",
        "sentry/sentry-laravel": "*",
        "tucker-eric/eloquentfilter": "*"
    },
    "require-dev": {
        "ext-intl": "*",
        "brianium/paratest": "*",
        "hotmeteor/spectator": "*",
        "laravel/dusk": "*",
        "mockery/mockery": "*",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-laravel": "*",
        "pestphp/pest-plugin-parallel": "*",
        "rector/rector": "*",
        "zircote/swagger-php": "*"
    },
    "replace": {
        "dragonmantank/cron-expression": "*",
        "league/commonmark": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "apcu-autoloader": true,
        "classmap-authoritative": false,
        "discard-changes": true,
        "htaccess-protect": false,
        "notify-on-install": false,
        "optimize-autoloader": true,
        "platform-check": false,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "openapi": "openapi --version 3.1.0 --output storage/openapi.yaml app"
    }
}
